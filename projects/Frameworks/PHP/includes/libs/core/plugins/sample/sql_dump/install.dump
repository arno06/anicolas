CREATE TABLE IF NOT EXISTS `main_sample` (
  `id_sample` int(11) NOT NULL AUTO_INCREMENT,
  `vc_sample` varchar(255) NOT NULL,
  `text_sample` text NOT NULL,
  `richtext_sample` text NOT NULL,
  `cb_sample` int(1) NOT NULL,
  `id_upload_sample` int(11) NOT NULL,
  PRIMARY KEY (`id_sample`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=1;